{
  "name": "score-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@xstate/react": "^1.0.1",
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "3.4.3",
    "xstate": "^4.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:browser-none": "BROWSER=none react-scripts start",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "docker:clear": "docker stop score-api mongo && docker rm score-api mongo",
    "docker:dev": "docker-compose --env-file config/.env.development up -d --force-recreate",
    "docker:staging": "docker-compose --env-file config/.env.staging up -d --force-recreate",
    "test:e2e": "start-server-and-test start:browser-none http://localhost:3000 cy:run",
    "test:e2e-full": ". scripts/test.e2e.sh",
    "lint": "./node_modules/.bin/eslint src/**/*.jsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^5.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "2.1.2",
    "start-server-and-test": "^1.11.5"
  }
}
